
<div class="container">
  <div class="row">
    <div class="col-sm-4 col-sm-offset-4">
      <%= simple_form_for [@product, @products_stock] do |f| %>
        <%= f.input :stand, collection: ProductsStock.stands %>
        <%= f.input :quantity %>
        <%= f.button :submit %>
        <%= link_to "Cancelar", products_path %>
      <% end %>
    </div>
  </div>
</div>
