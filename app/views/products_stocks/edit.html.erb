
<div class="container">
  <div class="row">
    <div class="col-sm-4 col-sm-offset-4">
      <%= simple_form_for @products_stock do |f| %>
        <%= f.association :stand, collection: ProductsStock.all.collect { |stock| stock.stand } %>
        <%= f.input :quantity %>
        <%= f.button :submit %>
        <%= link_to "Cancelar", products_path %>
      <%end%>
    </div>
  </div>
</div>
